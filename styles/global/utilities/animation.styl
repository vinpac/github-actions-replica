.collapseable
  transition: height 0.35s ease

.animate-slideInUp
  animation-name: slideInUp
  animation-duration: .5s
  animation-fill-mode: both

@keyframes slideInUp
  from
    transform: translate3d(0, 5%, 0)
    visibility: visible
    opacity 0

  to
    transform: translate3d(0, 0, 0)
    opacity 1
